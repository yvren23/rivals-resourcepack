{
  "format_version": "1.10.0",
  "particle_effect": {
    "description": {
      "identifier": "cobblemon:key_stone_god_rays",
      "basic_render_parameters": {
        "material": "particles_blend",
        "texture": "textures/particles/lightbeam"
      }
    },
    "components": {
      "minecraft:emitter_local_space": {
        "position": true,
        "rotation": false
      },
      "minecraft:emitter_rate_steady": {
        "spawn_rate": 12,
        "max_particles": 100
      },
      "minecraft:emitter_lifetime_once": {
        "active_time": 3
      },
      "minecraft:emitter_shape_point": {
        "offset": [0, 4, 0]
      },
      "minecraft:particle_lifetime_expression": {
        "max_lifetime": 1
      },
      "minecraft:particle_initial_spin": {
        "rotation": "v.emitter_age/v.emitter_lifetime*360*6 + v.emitter_age/v.emitter_lifetime*360*math.pi + v.emitter_random_1*360",
        "rotation_rate": "math.random(-20,20)"
      },
      "minecraft:particle_initial_speed": 0,
      "minecraft:particle_motion_dynamic": {},
      "minecraft:particle_appearance_billboard": {
        "size": [
          "(6/128) * 1 * v.particle_age * math.clamp(q.entity_size,1,2)",
          "2 * v.particle_age * math.clamp(q.entity_size,1,2)"
        ],
        "facing_camera_mode": "rotate_xyz",
        "uv": {
          "texture_width": 6,
          "texture_height": 128,
          "uv": [0, 0],
          "uv_size": [6, 128]
        }
      },
      "minecraft:particle_appearance_tinting": {
        "color": {
          "interpolant": "v.particle_age / v.particle_lifetime",
          "gradient": {
            "0.0": "#0037FFF7",
            "0.28": "#FFBC46FF",
            "0.5": "#FF64B9FF",
            "0.74": "#FFFF6262",
            "1.0": "#0082F8FF"
          }
        }
      }
    }
  }
}
