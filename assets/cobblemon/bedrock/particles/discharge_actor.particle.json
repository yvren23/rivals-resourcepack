{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "cobblemon:discharge_actor",
			"basic_render_parameters": {
				"material": "particles_blend",
				"texture": "textures/particles/electricity"
			}
		},
		"curves": {
			"variable.scale": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0, 1, 0.77, 0]
			}
		},
		"events": {
			"event_sound": {
				"sound_effect": {
					"event_name": "move.discharge.actor"
				}
			}
		},
		"components": {
			"minecraft:emitter_rate_instant": {
				"num_particles": 50
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 0.9
			},
			"minecraft:emitter_lifetime_events": {
				"creation_event": "event_sound"
			},
			"minecraft:emitter_shape_point": {
				"offset": ["math.random(-0.1,0.1)", "math.random(-0.1,0.1)", "math.random(-0.1,0.1)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 0.5
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random(0,360)"
			},
			"minecraft:particle_initial_speed": 10,
			"minecraft:particle_motion_dynamic": {},
			"minecraft:particle_appearance_billboard": {
				"size": ["1.5*variable.scale", "1.5*variable.scale"],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 16,
					"texture_height": 96,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [16, 16],
						"step_UV": [0, 16],
						"frames_per_second": 12,
						"max_frame": 6
					}
				}
			},
			"minecraft:particle_appearance_tinting": {
				"color": {
					"interpolant": "v.particle_age / v.particle_lifetime",
					"gradient": {
						"0.0": "#FFFBF236",
						"0.83": "#FFFF5A00"
					}
				}
			}
		}
	}
}